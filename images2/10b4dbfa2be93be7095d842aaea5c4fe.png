<script language="javascript">
function CheckForm()
{
var mobile = document.zhimatong.mobile.value;
if (mobile.length == 0) {
alert("请输入手机号码!");
document.zhimatong.mobile.focus();
return false;
}
if (mobile.length !== 11) {
alert("请输入有效的手机号码!");
document.zhimatong.mobile.focus();
return false;
}
if(!mobile.match(/^1[3|4|5|8][0-9]\d{4,8}$/)){
alert("请输入有效的手机号码!");
document.zhimatong.mobile.focus();
return false;
}
var email = document.zhimatong.email.value;
if (email.length == 0) {
alert("请输入电子邮箱!");
document.zhimatong.email.focus();
return false;
}
if(!email.match(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/)){
alert("请输入正确的电子邮箱!");
document.zhimatong.email.focus();
return false;
}
return true;
}
